-- Scripts:

local function NOWXACC_fake_script() -- INFINITEYIELD.inf yield script 
	local script = Instance.new('LocalScript', INFINITEYIELD)

	script.Parent.MouseButton1Click:Connect (function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end)
end
coroutine.wrap(NOWXACC_fake_script)()
local function ZXKNDU_fake_script() -- OWLHUB.owl hub 
	local script = Instance.new('LocalScript', OWLHUB)

	script.Parent.MouseButton1Click:Connect (function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))();
	end)
		
end
coroutine.wrap(ZXKNDU_fake_script)()
